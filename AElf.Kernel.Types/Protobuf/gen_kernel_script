#!/bin/sh
# script for generating the cs classes associated with the proto definitions.
# The generated code is placed in the Generated folder with a ".g.cs" extension


folder="Generated"
if [ ! -d "$folder" ]; then
  mkdir -p "$folder"
fi
protoc --proto_path=Proto --csharp_out=$folder --csharp_opt=file_extension=.g.cs Proto/kernel.proto Proto/hash.proto Proto/SignedIntegers.proto
