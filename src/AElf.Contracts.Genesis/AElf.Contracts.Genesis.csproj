<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageId>AElf.Contracts.Genesis</PackageId>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="..\AElf.Sdk.CSharp\AElf.Sdk.CSharp.csproj"/>
    </ItemGroup>

    <Import Project="$(SolutionDir)\AElf.Contract.Tools.targets"/>

    <ItemGroup>
        <ContractBase Include="$(SolutionDir)\protobuf\acs0.proto">
            <Link>Protobuf\Proto\acs0.proto</Link>
        </ContractBase>
        <ContractCode Include="$(SolutionDir)\protobuf\basic_contract_zero.proto">
            <Link>Protobuf\Proto\basic_contract_zero.proto</Link>
        </ContractCode>
    </ItemGroup>
</Project>