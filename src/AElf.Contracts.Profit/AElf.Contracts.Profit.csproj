<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageId>AElf.Contracts.Profit</PackageId>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\AElf.Sdk.CSharp\AElf.Sdk.CSharp.csproj" />
    </ItemGroup>

    <Import Project="$(SolutionDir)\AElf.Contract.Tools.targets" />

    <ItemGroup>
        <ContractReference Include="$(SolutionDir)\protobuf\acs0.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\reference\acs0.proto</Link>
        </ContractReference>
        <ContractReference Include="$(SolutionDir)\protobuf\token_contract.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\reference\token_contract.proto</Link>
        </ContractReference>
    </ItemGroup>
    <ItemGroup>
        <ContractCode Include="$(SolutionDir)\protobuf\profit_contract.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\profit_contract.proto</Link>
        </ContractCode>
    </ItemGroup>
</Project>
