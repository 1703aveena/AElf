<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp2.2</TargetFramework>
        <PackageId>AElf.Contracts.TestContract.BasicUpdate</PackageId>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <RootNamespace>AElf.Contracts.TestContract.BasicUpdate</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\AElf.Sdk.CSharp\AElf.Sdk.CSharp.csproj" />
    </ItemGroup>

    <Import Project="$(SolutionDir)\AElf.Contract.Tools.targets" />

    <ItemGroup>
        <ContractReference Include="$(SolutionDir)\protobuf\acs0.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\reference\acs0.proto</Link>
        </ContractReference>
    </ItemGroup>
    <ItemGroup>
        <ContractCode Include="$(SolutionDir)\protobuf\test_basic_update_contract.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\test_basic_update_contract.proto</Link>
        </ContractCode>
    </ItemGroup>

</Project>
