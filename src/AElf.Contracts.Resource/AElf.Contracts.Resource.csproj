<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="$(SolutionDir)\AElf.Contract.Tools.targets"/>
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageId>AElf.Contracts.Resource</PackageId>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="..\AElf.Sdk.CSharp\AElf.Sdk.CSharp.csproj"/>
    </ItemGroup>
    <ItemGroup>
        <ContractReference Include="$(ProtoBaseDir)\token_contract.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\reference\token_contract.proto</Link>
        </ContractReference>
    </ItemGroup>
    <ItemGroup>
        <ContractCode Include="$(ProtoBaseDir)\resource_contract.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\resource_contract.proto</Link>
        </ContractCode>
    </ItemGroup>
</Project>