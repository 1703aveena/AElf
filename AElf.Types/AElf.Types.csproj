<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
    </PropertyGroup>
    <ItemGroup>
      <PackageReference Include="Google.Protobuf" Version="3.6.0" />
    </ItemGroup>
    <Target Name="Gen_proto" BeforeTargets="BeforeBuild">
        <Exec Command="protoc --proto_path=../AElf.Types/Protobuf/Proto --csharp_out=./Protobuf/Generated --csharp_opt=file_extension=.g.cs common.proto" />
    </Target>
</Project>
